{
	"$schema": "https://typedoc.org/schema.json",
	"entryPoints": ["lib/src/misskey.d.ts", "lib/src/std.d.ts"],
	"out": "AiScriptDocs/docs/api/doc",
	"name": "AiScript APIリファレンス",
	"tsconfig": "lib/src/tsconfig.json",
	"skipErrorChecking": true,
	"plugin": ["typedoc-plugin-markdown", "typedoc-plugin-frontmatter", "./dist/mdxReplacer.js", "./dist/aiScriptConvert.js"],
	"hidePageHeader": true,
	"hideBreadcrumbs": true,
	"useCodeBlocks": true,
	"parametersFormat": "table",
	"propertiesFormat": "table",
	"textContentMappings": {
		"header.title": "{projectName} {version}",
		"header.readme": "Readme",
		"header.docs": "API",
		"breadcrumbs.home": "{projectName} {version}",
		"title.indexPage": "{projectName} {version}",
		"title.modulePage": "{name}",
		"title.memberPage": "{name}",
		"label.defaultValue": "初期値",
		"label.description": "説明",
		"label.flags": "フラグ",
		"label.globals": "グローバル",
		"label.index": "Index",
		"label.indexable": "Indexable",
		"label.indexSignature": "Index signature",
		"label.member": "メンバー",
		"label.modifier": "Modifier",
		"label.name": "名前",
		"label.overrides": "Overrides",
		"label.packages": "Packages",
		"label.reExports": "Re-exports",
		"label.renamesAndReExports": "Renames and re-exports",
		"label.returns": "返り値",
		"label.source": "Source",
		"label.type": "型",
		"label.typeDeclaration": "Type declaration",
		"label.value": "デフォルト値",
		"kind.accessor.singular": "Accessor",
		"kind.accessor.plural": "Accessors",
		"kind.class.singular": "プリミティブプロパティ",
		"kind.class.plural": "プリミティブプロパティ",
		"kind.constructor.singular": "Constructor",
		"kind.constructor.plural": "Constructors",
		"kind.enum.singular": "Enumeration",
		"kind.enum.plural": "Enumerations",
		"kind.enumMember.singular": "Enumeration Member",
		"kind.enumMember.plural": "Enumeration Members",
		"kind.event.singular": "Event",
		"kind.event.plural": "Events",
		"kind.function.singular": "関数",
		"kind.function.plural": "関数",
		"kind.interface.singular": "インターフェース",
		"kind.interface.plural": "インターフェース",
		"kind.method.singular": "関数",
		"kind.method.plural": "関数",
		"kind.module.singular": "Module",
		"kind.module.plural": "Modules",
		"kind.namespace.singular": "名前空間",
		"kind.namespace.plural": "名前空間",
		"kind.variable.singular": "変数",
		"kind.variable.plural": "変数",
		"kind.parameter.singular": "引数",
		"kind.parameter.plural": "パラメーター",
		"kind.property.singular": "プロパティ",
		"kind.property.plural": "プロパティ",
		"kind.reference.singular": "Reference",
		"kind.reference.plural": "References",
		"kind.typeAlias.singular": "定数タイプ",
		"kind.typeAlias.plural": "定数タイプ",
		"kind.typeParameter.singular": "型引数",
		"kind.typeParameter.plural": "型引数",
		"label.implements": "Implements",
		"label.implementationOf": "Implementation of",
		"label.inheritedFrom": "Inherited from",
		"label.extendedBy": "継承もと",
		"label.extends": "継承"
	},
	"blockTags": [
		"@deprecated",
		"@param",
		"@remarks",
		"@returns",
		"@throws",
		"@privateRemarks",
		"@defaultValue",
		"@typeParam",
		"@module",
		"@inheritDoc",
		"@group",
		"@groupDescription",
		"@category",
		"@categoryDescription",
		"@template",
		"@type",
		"@typedef",
		"@callback",
		"@prop",
		"@property",
		"@satisfies",
		"@aiPrimitive"
	],
	"modifierTags": [
		"@public",
		"@private",
		"@protected",
		"@internal",
		"@readonly",
		"@packageDocumentation",
		"@eventProperty",
		"@alpha",
		"@beta",
		"@experimental",
		"@sealed",
		"@override",
		"@virtual",
		"@hidden",
		"@ignore",
		"@class",
		"@enum",
		"@event",
		"@overload",
		"@namespace",
		"@interface",
		"@showCategories",
		"@hideCategories",
		"@showGroups",
		"@hideGroups",
		"@aiNamespace"
	]
}
